@model RandomImage[]
@{
    ViewData["Title"] = "Home Page";
}

@foreach (var currImage in Model)
{
    <div class="card" style="width: 18rem;">
        <img src="@currImage.url" class="card-img-top" alt="Photo for @currImage.id">
        <div class="card-body">
            <h5 class="card-title">Random Cat</h5>
            <p class="card-text">Image of id:@currImage.id</p>
            <a href="#" class="btn btn-primary">Get details</a>
        </div>
    </div>
}